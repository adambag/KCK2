<Window x:Class="KCK2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KCK2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/main.png"/>
        </Grid.Background>


        <Frame x:Name="MainContentFrame" Visibility="Collapsed" />
        <TextBlock Name="StartView" HorizontalAlignment="Center" FontSize="30" Text="Sword and Sandals 0.5" VerticalAlignment="Center"/>
        <Button Name="StartButton" Content="Start" HorizontalAlignment="Center" Height="31" FontSize="20" VerticalAlignment="Top" Width="147" Margin="0,273,0,0" Click="StartButton_Click"/>
        
    </Grid>
</Window>
